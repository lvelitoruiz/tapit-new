{
  "name": "home",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "b": "ng build -c=dev --output-path=../dist/home",
    "b-qa": "ng build -c=qa --output-path=../dist/home --base-href=/home/",
    "dev:ssr": "ng run home:serve-ssr",
    "serve:ssr": "node dist/home/server/main.js",
    "build:ssr": "ng build --prod && ng run home:server:production",
    "serve:ssrdev": "ng build -c=dev && ng run home:server:dev && npm run serve:ssr",
    "serve:ssrqa": "ng build -c=qa && ng run home:server:qa && npm run serve:ssr",
    "prerender": "ng run home:prerender"
  },
  "private": true,
  "dependencies": {
