{
  "hosting": [
    {
      "target": "dev",
      "public": "dist/ABInBevWeb",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "redirects": [
        {
          "source": "*",
          "destination": "https://home.gettap.it",
          "type": 301
        }
      ]
    },
    {
      "target": "prod",
      "public": "dist/ABInBevWeb",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "redirects": [
        {
          "source": "*",
          "destination": "https://home.gettap.it",
          "type": 301
        }
      ]
    }
  ]
}
